{
  "name": "dataclass",
  "version": "1.1.1",
  "description": "Immutable data transfer objects for JavaScript",
  "author": "Alexey Raspopov",
  "license": "MIT",
  "homepage": "https://dataclass.js.org/",
  "repository": "alexeyraspopov/dataclass",
  "main": "build/dataclass.js",
  "module": "build/dataclass.module.js",
  "types": "typings/dataclass.d.ts",
  "files": [
    "build/",
    "typings/",
    "LICENSE"
  ],
  "keywords": [
    "immutable",
    "value-objects",
    "data-structures",
    "typings"
  ],
  "scripts": {
    "test": "jest --coverage",
    "ci": "sh scripts/integration.sh",
    "prepublish": "./scripts/publish.sh"
  },
  "devDependencies": {
    "babel-core": "~6.25.0",
    "babel-jest": "^22.4.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "babel-plugin-transform-flow-strip-types": "~6.22.0",
    "babel-preset-env": "^1.6.1",
    "jest": "^22.4.3",
    "jest-junit": "^3.6.0",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "size-limit": "^0.17.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "8.9.4"
          }
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "transform-flow-strip-types"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "size-limit": [
    {
      "limit": "457 B",
      "path": "build/dataclass.js"
    },
    {
      "limit": "465 B",
      "path": "build/dataclass.module.js"
    }
  ]
}
